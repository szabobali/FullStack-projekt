@model IEnumerable<string>
<a class="btn btn-block btn-secondary” asp-controller="Home" asp-action="Index"
   asp-route-category="">Home</a>
@foreach (string category in Model)
{
	<a class="btn btn-block btn-secondary" asp-controller="Home" asp-action="Index"
	   asp-route-category="@category">@category</a>
}